package com.myspace.spectrum;

import java.lang.Number;

rule "Valet Services"
	dialect "mvel"
	when
		Patient( isPregnant == true , hasDisability == true , age > 65 , appointment.facility.hasValet == true , appointment.date > "06-Aug-2019" , hasUsedPortal == true )
	then
		insert( new NotificationOfValetServices() );
end
