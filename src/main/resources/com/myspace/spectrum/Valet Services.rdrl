package com.myspace.spectrum;

import java.lang.Number;

rule "Valet"
	dialect "mvel"
	when
		$patient : Patient( isPregnant == true , hasDisability == true , age > 65 , appointment.facility.hasValet == true , hasUsedPortal == true , appointment.scheduled == true , appointment.daysToAppointment == 1 )
	then
end
